<h1>Sign-in</h1>
<form action="<%=session_url%>" method="post">
  <%= auth_token %>
  <label>Email:
    <input type="text" name="session[email]" id='Email'>
  </label>
  <label>Password:
    <input type="password" name="session[password]" id='Password'>
  </label>
  <input type="submit" value="Sign-in">
</form>
